<% if (typeof profile !== 'undefined') { %>
	<% const profileJSON = JSON.parse(profile); %>
	<h1><%= profileJSON.username %></h1>
	<% if (profileJSON.quotes && profileJSON.quotes.length !== 0) { %>
		<h3>quotes</h3>
		<ul class="collection">
			<% profileJSON.quotes.forEach((quote) => { %>
				<% include ../quotes/quote %>
			<% }); %>
		</ul>
		<% if (currUser.displayName === profileJSON.username) { %>
			<script> const userQuotes = <%- profile %>.quotes; </script>
			<script src="/js/curr_user_btns.js" defer></script>
		<% } else { %>
			<script> const nonUserQuotes = <%- profile %>.quotes; </script>
			<script src="/js/voting_btns.js" defer></script>
		<% } %>
	<% } else { %>
		<h4>No quotes &#58;&#40;</h4>
	<% } %>
<% } else { %>
	<h1>Could not get profile</h1>
<% } %>